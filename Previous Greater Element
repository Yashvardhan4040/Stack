#include<iostream>
using namespace std;
void previousgreater(int arr[], int n)
{
    for(int i=0;i<n;i++)
    {
        int j;
        for(j=i-1;j>=0;j--)
        {
            if(arr[j]>arr[i])
            {cout<<arr[j]<<" ";
            break;
        }
    }
    if(j==-1)
    cout<<-1<<" ";
}
}
int main() 
{ 
    int arr[]={18,12,13,14,11,16};
    int n=6;
    previousgreater(arr,n);
    return 0; 
}

